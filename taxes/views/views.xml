<odoo>
  <data>
    <record model="ir.ui.view" id="account_move_form_inherit">
        <field name="name">account.move.form.inherit</field>
        <field name="model">account.move</field>
        <field name="inherit_id" ref="account.view_move_form"/>
        <field name="arch" type="xml">
            <xpath expr="//header"
                   position="inside">
                <button name="apply_aisi" string="Appliquer AISI" type="object" class="btn-primary"/>
            </xpath>
            <xpath expr="//field[@name='tax_cash_basis_origin_move_id']"
                   position="after">
                <field name="is_aisi" string="AISI" class="oe_inline"/>
            </xpath>
        </field>
    </record>
  </data>
</odoo>